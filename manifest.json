      "targets": [ "ms.vss-distributed-task.tasks" ],
      "properties": {
        "name": "AddNugetSources"
      }
    }
  ],
  "files": [
    {
      "path": "AddNugetSources"
    },
    {
      "path": "Images/package_600x600.jpg", "addressable": true
    }
  ]
}